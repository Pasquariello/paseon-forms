{
  "hosting": {
    "public": "build/uncompiled-unbundled/",
    "rewrites": [
      {
        "source": "**/!(*.*)",
        "destination": "/src/index.js"
      }
    ],     
    "headers": [ {
      "source" : "**",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
    }]
  }
}
